{
  "extends": ["solhint:recommended"],
  "rules": {
    "compiler-version": ["error", "0.8.27"],
    "no-empty-blocks": "error",
    "no-inline-assembly": "warn",
    "func-name-mixedcase": "off",
    "max-states-count": ["warn", 15],
    "func-visibility": ["error", { "ignoreConstructors": true }]
  }
}
